{
  "name": "lpr-frontend",
  "version": "1.0.0",
  "description": "License Plate Recognition System Frontend",
  "scripts": {
    "build:css": "npx tailwindcss -i ./frontend/static/css/tailwind.css -o ./frontend/static/css/output.css --minify",
    "watch:css": "npx tailwindcss -i ./frontend/static/css/tailwind.css -o ./frontend/static/css/output.css --watch",
    "dev": "npm run watch:css"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  },
  "keywords": [
    "license-plate",
    "recognition",
    "ocr",
    "yolo"
  ],
  "author": "",
  "license": "MIT"
}
